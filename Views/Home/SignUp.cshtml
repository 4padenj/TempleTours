@{
}
@model DayOfWeekViewModel
<div class="text-center">
    <h1 class="display-4">Available Times</h1>

    <div>
        <div>
            <h3>Monday</h3>

            @foreach (var x in Model.Monday)
            {
                <div>
                    <p><a asp-action="">@x.Time</a></p>
                </div>
            }
        </div>   
    </div>
    
</div>
